    <!-- Work -->
    <article id="work" class="wrapper style2">
        <div class="container">
            <header>
                <h2 style="color:white">Songs.</h2>
                <p style="color:#4799eb">List of recently shared music.</p>
            </header>
            <div class="row aln-center">
                <% for(var i = songs.length - 1; i >= 0 ; i --){ %>
                <div class="col-12 col-12-medium col-12-small">
                    <a href="/music/<%= songs[i]._id %>" style="text-decoration:none">
                        <section class="box style1 animated fadeIn wow">
                            <!-- <span class="icon featured fa-comments-o"></span> -->
                            <h3>
                                <%= songs[i].username %> <br>
                                <%= songs[i].title %>
                            </h3>
                            <p>
                                <%= songs[i].short_description %>
                            </p>
                            <a href="/music/<%= songs[i]._id %>" style="background: purple;
						padding: 2px 5px;
						border-radius: 5px;text-decoration: none;color: white;font-weight: 600">Listen
                                to song</a>
                            <br>
                            <span class="icon fa-thumbs-o-up" style="
								color: palegreen;
								margin: 6px;
							">
                                <%= songs[i].likes %></span>
                            <% for(var l = 0; l < songs[i].genres.length ; l ++){ %>
                            <span style="background:tomato" class="genre">
                                <%= songs[i].genres[l] %></span>
                            <% } %>
                            <span style="color:palegreen">
                                <%= songs[i].views %> </span>Views
                        </section>
                    </a>
                </div>
                <% } %>
            </div>
